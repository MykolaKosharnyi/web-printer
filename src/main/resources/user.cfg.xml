<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nk.printmaster.entities.User" table="user">
		<id name="id" column="id">
			<generator class="native"/>
		</id>
		<property name="firstName" column="first_name" type="string"/>
		<property name="secondName" column="second_name" type="string"/>
		<property name="phoneNumber" column="phone_number" type="int"/>
		<property name="company" column="company" type="string"/>
		<property name="email" column="email" type="string"/>
	</class>
</hibernate-mapping>